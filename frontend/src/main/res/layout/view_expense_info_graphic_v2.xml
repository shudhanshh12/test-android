<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <ImageView
        android:id="@+id/info_img_v2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.6"
        android:layout_marginTop="20dp"
        android:padding="4dp"
        android:background="@color/green_lite"
        android:visibility="gone"
        tools:visibility="visible"
        android:src="@drawable/ic_expense_info_graphic_2"
        app:layout_constraintTop_toBottomOf="@+id/youtube_thumbnail"
        app:layout_constraintBottom_toTopOf="@+id/info_description"/>

</merge>
